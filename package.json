{
  "name": "courier-validation",
  "type": "module",
  "version": "0.0.3-beta.3",
  "description": "A lightweight library for validating and formatting courier tracking numbers",
  "author": "BF-zh<2581807417@qq.com>",
  "homepage": "https://github.com/BF-zh/Courier-Validation#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BF-zh/Courier-Validation.git"
  },
  "bugs": "https://github.com/BF-zh/Courier-Validation/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.7.1",
    "@antfu/ni": "^0.23.0",
    "@antfu/utils": "^0.7.10",
    "@types/node": "^22.5.5",
    "bumpp": "^9.5.2",
    "eslint": "^8.57.1",
    "esno": "^4.7.0",
    "lint-staged": "^15.2.10",
    "pnpm": "^9.11.0",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.6.2",
    "unbuild": "^2.0.0",
    "vite": "^5.4.7",
    "vitest": "^2.1.1"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
